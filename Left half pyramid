public class Solution {
    public static void main(String[] args) {
    int n = 5; #declaration no of lines
    for(int i=1;  i<=n; i++){
        for(int j=1; j<=n-i; j++){ #for creating space
            System.out.print(" "); 
        }
        for(int k=1;k<=i;k++) {
            System.out.print("*");
        }
        System.out.println();
    }

}
}

